\name{BERGSNPs}
\alias{BERGSNPs}
\docType{data}
\title{
List of all the markers included in BERG genomics studies
}
\description{
A total of 5,093,674 individual SNPs are listed with information on where to find them.
}
\usage{data("BERGSNPs")}
\format{
  A list with 32 elements
  \describe{
    \item{\code{ICOGS}}{SNP-list  for ICOGS}
    \item{\code{BRCA_CGEMS}}{SNP-list  for CGEMS breast}
    \item{\code{ERNEG}}{SNP-list  for ER- breast cancer GWAS (BPC3)}
    \item{\code{BCAC}}{SNP-list  for BCAC  GWAS (breast Oncoarray)}
    \item{\code{BRCA_TAQ}}{SNP-list  for Breast cancer taqman data}
    \item{\code{AAPC}}{SNP-list  for African American Prostate Cancer GWAS (AAPC)}
    \item{\code{AGGPRCA}}{SNP-list  for Aggressive Prostate GWAS}
    \item{\code{PRCA_TAQ_CGF1}}{SNP-list  for CGF Prostate Taqman data - 1st wave}
    \item{\code{PRCA_TAQ_CGF2}}{SNP-list  for CGF Prostate Taqman data - 2nd wave}
    \item{\code{PRCA_TAQ_CGF3}}{SNP-list  for CGF Prostate Taqman data - 3rd wave}
    \item{\code{PRCA_TAQ_USC}}{SNP-list  for USC Prostate Taqman data}
    \item{\code{PRCA_CGEMS1}}{SNP-list  for CGEMS prostate - 1st wave}
    \item{\code{PRCA_CGEMS2_BLOOD}}{SNP-list  for CGEMS prostate - 2nd wave (blood)}
    \item{\code{PRCA_CGEMS2_BUCCAL}}{SNP-list  for CGEMS prostate - 2nd wave (buccal)}
    \item{\code{BLADDER}}{SNP-list  for Bladder GWAS}
    \item{\code{NHL}}{SNP-list  for NHL Gwas}
    \item{\code{CORECT}}{SNP-list  for CORECT GWAS}
    \item{\code{GECCO_EXCOME}}{SNP-list  for GECCO Exome}
    \item{\code{GECCO_ONCOARRAY}}{SNP-list  for GECCO (oncoarray GWAS)}
    \item{\code{E2C2}}{SNP-list  for E2C2 (Endometrium)}
    \item{\code{GLIOMA}}{SNP-list  for Glioma GWAS}
    \item{\code{LUNG_COMBINED}}{SNP-list  for Lung cancer (BLK GWAS)}
    \item{\code{LUNG_1M_CONTROL}}{SNP-list  for Lung cancer (BLK GWAS)}
    \item{\code{LYM_SUPER}}{SNP-list  for Lymphoma super controls (GWAS)}
    \item{\code{TGS}}{SNP-list  for NCI Total GWAS set}
    \item{\code{NICOTINE}}{SNP-list  for Nicotine SNPs}
    \item{\code{PANSCAN1}}{SNP-list  for PANSCAN-1 (GWAS)}
    \item{\code{PANSCAN2}}{SNP-list  for PANSCAN-2 (GWAS)}
    \item{\code{PRCA_ONCOARRAY}}{SNP-list  for Prostate GWAS (Oncoarray)}
    \item{\code{RCC_GWAS}}{SNP-list  for Renal Cell Carcinoma GWAS}
    \item{\code{RENAL_GWAS}}{SNP-list  for Renal GWAS}
    \item{\code{Summary}}{Summary table listing all studies and their locations}
  }
}
